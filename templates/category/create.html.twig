{% extends "base.html.twig" %}

{% block title %}
	Création d'une catégorie !
{% endblock %}

{% block body %}
	<h1>créer une catégorie de produit</h1>

	{{ form_start(formView) }}
	<div class="row">
		<div class="col">
			{{ form_row(formView.name)}}

		</div>
		<div class="col"></div>
	</div>

	{{ form_errors(formView) }}

	<button type="submit" class="btn btn-primary">
		<i class="fas fa-save"></i>&nbsp;Creer la catégorie
	</button>
	{{ form_end(formView)}}
{% endblock %}
